{
  "$schema": "https://vega.github.io/schema/vega-lite/v2.json",
  "description": "Quantile chart",
  "mark": "line",
  "autosize": {
    "type": "fit",
    "contains": "padding"
  },
  "encoding": {
    "x": {
      "field": "quantile",
      "type": "quantitative",
      "axis": {
        "title": "",
        "format": ".0%"
      }
    },
    "y": {"field": "visitors", "type": "quantitative"}
  },
  "config": {
    "mark": {
      "color": "#990000"
    }
  }
}
